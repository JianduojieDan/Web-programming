    <script>
        window.onload = function() {
            let fruits = [];

            fruits.push("Apple");
            fruits.push("Banana");
            fruits.push("Cherry");

            let removedFruit = fruits.pop();

            console.log("Final array:", fruits);
            console.log("Removed fruit:", removedFruit);
        };
    </script>
