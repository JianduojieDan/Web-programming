<body>
<script>
function parseJSON(jsonStr) {
    try {
        let data = JSON.parse(jsonStr);
        console.log(data);
    } catch (error) {
        console.error("Invalid JSON format");
    }
}

parseJSON('{"name": "Alice", "age": 25}');
parseJSON('{name: Alice, age: 25}');
</script>
</body>
